# コマンド履歴
HISTFILE=~/.zsh_history
HISTSIZE=50000
SAVEHIST=50000
setopt hist_ignore_dups     # ignore duplication command history list
setopt share_history        # share command history data

# alias
alias py="python"
alias es="elasticsearch"
alias vi="vim"
alias be="bundle exec"
alias ber="bundle exec rspec"
